{
  "name": "marvel-searcher",
  "version": "1.0.0",
  "description": "Marvel's Characters searcher powered by Marvel Public API.",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "start": "npm run build && npm run server",
    "server": "node server.js",
    "build": "node node_modules/webpack/bin/webpack.js --config webpack/production.config.js",
    "dev": "webpack-dev-server --config webpack/development.config.js --open",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx}\"",
    "lint:css": "stylelint \"src/**/*.{scss,css}\"",
    "format": "prettier \"src/**/*.{js,jsx}\"",
    "format:fix": "prettier --check --write \"src/**/*.{js,jsx}\"",
    "pre:commit": "npm run lint:fix && npm run format:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'pre-commit script goes here'"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.2",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "prettier": "^2.1.1",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.2"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "express": "^4.17.1",
    "md5": "^2.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1"
  }
}