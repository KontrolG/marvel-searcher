{
  "ignorePatterns": [
    "node_modules/"
  ],
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": "warn",
    "quotes": "off",
    "no-console": "warn",
    "consistent-return": "off",
    "func-names": "off",
    "object-shorthand": [
      "warn",
      "always"
    ],
    "no-process-exit": "off",
    "no-param-reassign": [
      "error",
      {
        "props": true
      }
    ],
    "no-underscore-dangle": "off",
    "prefer-destructuring": [
      "error",
      {
        "object": true,
        "array": false
      }
    ],
    "no-undef": [
      "warn",
      {
        "typeof": false
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "request|response|next|value|props"
      }
    ]
  }
}